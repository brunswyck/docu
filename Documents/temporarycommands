root@debian:/home/patrick/Downloads/firefox_dlds# chown root:root /usr/lib/flashplugin-nonfree/libflashplayer.so 

root@debian:/home/patrick/Downloads/firefox_dlds# update-alternatives --list flash-mozilla.so/usr/lib/flashplugin-nonfree/libflashplayer.so


